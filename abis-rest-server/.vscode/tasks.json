{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "C/C++: GCC build",
			"command": "g++.exe",
			"args": [
				"-g",
				"${file}",
				"-o",
				"${fileDirname}\\${workspaceFolderBasename}.exe",
				"-Wall",
				"-Id:/bin/msys2/usr/include/",
                "-Id:/lib/vcpkg/installed/x64-windows/include/"

			],
			"options": {
				"cwd": "D:/bin/msys2/mingw64/bin/"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}